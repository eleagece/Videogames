#ifndef ObjetoCuadricoH
#define ObjetoCuadricoH
#include "Objeto3D.h"

class ObjetoCuadrico: public Objeto3D  // Clase hija que hereda de Objeto3D
 {                                     // Clase abstracta por tener un método virtual puro 'dibuja()'. No se pueden hacer instancias de la clase
 protected:
  // Atributos protegidos
  GLUquadricObj* objeto;  // Simplemente un GLUquadricObj

 public:
  // Constructoras y destructora
  ObjetoCuadrico();
  ~ObjetoCuadrico();
 };

#endif